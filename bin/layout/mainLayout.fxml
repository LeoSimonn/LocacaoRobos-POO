<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="500" prefWidth="900" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aplicacao.ACMERobots">
  <children>
    <MenuBar prefHeight="19.0" prefWidth="900">
      <menus>
        <Menu mnemonicParsing="false" text="Cadastros">
          <items>
            <MenuItem fx:id="novoClienteMenuItem" mnemonicParsing="false" text="Novo Cliente" />
            <MenuItem fx:id="novoRoboMenuItem" mnemonicParsing="false" text="Novo Robô" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Locações">
          <items>
            <MenuItem mnemonicParsing="false" text="Nova Locação" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Sair">
               <items>
                  <MenuItem  fx:id="sairMenuItem" mnemonicParsing="false" text="Sair do Sistema" />
               </items></Menu>
      </menus>
    </MenuBar>
    <SplitPane dividerPositions="0.3" prefHeight="503.0" prefWidth="907.0">
      <items>
        <AnchorPane fx:id="leftAnchorPane" prefHeight="500" prefWidth="270" />
        <AnchorPane fx:id="rightAnchorPane" prefHeight="500" prefWidth="630" />
      </items>
    </SplitPane>
  </children>
</VBox>
